<recipeItem>
   <name>Sopa de tomate</name>
   <type>starter</type>
   <picture>cukio_2015.04.05_20.11.47.jpg</picture>
   <ingredients class="java.util.ArrayList">
      <string>10 unidad/es tomates maduros</string>
      <string>1 unidad/es cebolla</string>
      <string>50 g albahaca</string>
      <string>250 ml agua</string>
      <string>150 g tomate frito</string>
      <string>25 g azúcar</string>
   </ingredients>
   <steps class="java.util.ArrayList">
      <string>Rallar los tomates maduros y añadir todos los ingredientes en el bol. Cocer a baja presión. Retirar del bol y colar. Tipo de cocción: dorar: 6 minutos.</string>
   </steps>
   <author>Programada en el robot</author>
   <vegetarian>true</vegetarian>
   <portions>2</portions>
   <minutes>11</minutes>
   <language>Spanish</language>
   <tip></tip>
</recipeItem>