<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content">

    <com.rukiasoft.androidapps.cocinaconroll.classes.DotsView
        android:id="@+id/vDotsView"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="center"/>

    <com.rukiasoft.androidapps.cocinaconroll.classes.CircleView
        android:id="@+id/vCircle"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="center"/>

    <ImageView
        android:id="@+id/ivStar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/ic_favorite_outline_white_36dp"
        android:contentDescription="@string/null_content_description"/>

</merge>